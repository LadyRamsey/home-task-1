package com.gmail.gunkophotos;

public class Main {

	public static void main(String[] args) {
		int[] a = { 3, 7, 1, 9, 51, 12, 43 };
		System.out.println( max (a));
	}

	public static int max(int[] array) {
		
		int max = array[0];
		
		for (int i = 0; i < array.length; i++) {
			if (array[i] > max) {
				max = array[i];
			}
		}
		return max;
	}
}
