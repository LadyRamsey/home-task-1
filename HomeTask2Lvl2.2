package com.gmail.gunkophotos;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.println("Input number of your ticket");

		int number = sc.nextInt();

		int a = (number / 1000) + (number % 1000 / 100);

		int b = (number % 100 / 10) + (number % 10);

		if (a == b) {
			System.out.println("Your ticket is lucky");
		} else {
			System.out.println("Your ticket isn`t lucky, try again)");
		}

	}

}
